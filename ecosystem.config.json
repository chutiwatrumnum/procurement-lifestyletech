{
  "apps": [
    {
      "name": "procurement-system",
      "script": "npm",
      "args": "run dev",
      "cwd": "/root/.openclaw/workspace/procurement-system",
      "autorestart": true,
      "watch": false,
      "max_memory_restart": "1G",
      "env": {
        "NODE_ENV": "development"
      },
      "error_file": "/root/.openclaw/workspace/procurement-system/logs/err.log",
      "out_file": "/root/.openclaw/workspace/procurement-system/logs/out.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss Z"
    }
  ]
}